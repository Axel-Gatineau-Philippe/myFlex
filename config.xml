<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.myflex" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>myFlex</name>
    <description>
        Calculez votre horaire de d√©part avec myFlex.
    </description>
    <author email="axel.gatineau.philippe@gmail.com" href="http://github.com/Axel-Gatineau-Philippe/">
        Axel GATINEAU PHILIPPE
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="./www/img/logo_size.jpg"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>